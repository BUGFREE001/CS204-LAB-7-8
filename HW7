#5


#include <stdio.h>

int main()
{
	int s,c;
	printf ("Enter size of array: ");
	scanf("%d", &s);
	
	int a[s],fre[s];
	printf ("Enter each element for the given array: ");
	for (int i=0; i<s; i++){
		scanf("%d", &a[i]);
		fre[i] = -1;
	}
  for(int i=0; i<s; i++){
    c = 1;
    for(int j=i+1; j<s; j++){
      if(a[i]==a[j]){
        c++;
        fre[j] = 0;
      }
    }
    if(fre[i]!=0){
      fre[i] = c;
    }
  }
  
  printf("Frequency for each element: ");
  for(int i=0; i<s; i++){
    if(fre[i]!=0){
      printf("%d-%d ", a[i], fre[i]);
    }
  }
	
  return 0;	


}




