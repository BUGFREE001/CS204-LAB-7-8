#3


#include <stdio.h>

int main()
{
	int n;
	printf ("Enter the number of lines you want to print: ");
	scanf("%d", &n);

  int c=1;
	for (int i=1; i<=n; i++)
	{
		for (int j=0; j<i; j++)
		{
          printf("%d ", c);
          c++;
		}
		printf("\n");
	}
    return 0;
}





