#2
 
#include<stdio.h>
 
void print(int *, int );

int main()
{
  int temp,i,j,n;
	printf ("Please give the size of the array: ");
	scanf("%d", &n);
  
  int m[n];
  printf("Enter each element for the given array: ");
    for (i = 0 ; i < n; i++){
      scanf("%d", &m[i]);
    } 
    printf("All permutations: ");
    for (j = 1; j <= n; j++) {
        for (i = 0; i < n-1; i++) {
            temp = m[i];
            m[i] = m[i+1];
            m[i+1] = temp;
            print(m, n);
	      }
    }
    return 0;
}

void print(int *m, int n)
{
    int i;
    for ( i = 0 ; i < n ; i++){
       printf("%d", m[i]);
    }
    printf(" ");
}




