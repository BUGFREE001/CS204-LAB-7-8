
#1

#include <stdio.h>
 
 void sort ();
 
 int main ()
 {  
	 int s;

	 printf ("Enter size of array: ");
	 scanf("%d", &s);
	 int a[s];
	 printf("Enter numbers for each element: ");
	 for (int i=0; i<s; i++){
		 scanf("%d", &a[i]);
	 }

	 sort (a, s);
   printf ("Results: ");
	 for (int i=0; i<s-1; i++){
	  if (a[i]<a[i+1]){
		  printf ("%d ", a[i]);
	  }
    if (i==s-2 && a[i]<=a[i+1]){
      printf("%d", a[i+1]);
    }
  }
	 return 0;
 }
 
 void sort (int a[], int s){
	 for (int i=1; i<s; i++){
		 for (int j=0; j<s-i; j++){
			int temp;
			if (a[j]>a[j+1]){
				temp = a[j];
				a[j] = a[j+1];
				a[j+1] = temp;
			}		 
		 }
	 }
 } 
 
 
 
 
 
