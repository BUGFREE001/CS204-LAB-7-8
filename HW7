#4

#include <stdio.h>
 
int check(char [], char []);
 
int main()
{
  char a[10000], b[10000];
  printf("Enter the first string:");
  gets(a);
  printf("And second string:");
  gets(b);
  printf("Results: ");
  if (check(a, b) == 1)
    printf("Yes\n");
  else
    printf("No\n");
    
  return 0;
}
 
int check(char a[], char b[])
{
  int fst[26] = {0}, snd[26] = {0}, c=0;
  while (a[c] != '\0'){
    fst[a[c]-'a']++;
    c++;
  }
  c = 0;
  while (b[c] != '\0'){
    snd[b[c]-'a']++;
    c++;
  }
  for (c = 0; c < 26; c++){
    if (fst[c] != snd[c])
      return 0;
  }
  return 1;
}




